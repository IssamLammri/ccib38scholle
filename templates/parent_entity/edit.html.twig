{% extends 'base.html.twig' %}

{% block title %}Edit Parent{% endblock %}

{% block body %}
    <edit-parent
            :parent='{{ parent_entity|json_encode|raw }}'
            :students='{{ students|json_encode|raw }}'
            update-url="{{ path("api_parent_update", { id: parent_entity.id }) }}"
            csrf-token="{{ csrf_token('edit_parent_' ~ parent_entity.id) }}"
    ></edit-parent>
{% endblock %}