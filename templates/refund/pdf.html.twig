<!DOCTYPE html>
<html lang="fr">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Remboursement N° {{ refund.id }}</title>
    <style>
        :root {
            --clr-bg: #ffffff;
            --clr-panel: #ffffff;
            --clr-primary: #000000;
            --clr-accent: #1e3c72;
            --clr-border: #e0e0e0;
            --clr-highlight: #f0f6ff;
            --ff-base: 'Helvetica Neue', Helvetica, Arial, sans-serif;
        }
        * { box-sizing: border-box; }
        body {
            margin: 0;
            padding: 20px;
            background: var(--clr-bg);
            font-family: var(--ff-base);
            color: var(--clr-primary);
        }
        .refund-container {
            max-width: 800px;
            margin: 0 auto;
            background: var(--clr-panel);
            border-radius: 8px;
            overflow: hidden;
            box-shadow: 0 2px 8px rgba(0,0,0,0.1);
        }
        .top-panel {
            display: flex;
            background: var(--clr-accent);
            color: #fff;
            padding: 16px;
            align-items: center;
        }
        .logo img {
            height: 50px;
        }
        .top-panel .middle {
            flex: 1;
            text-align: center;
            font-size: 1.2rem;
            font-weight: bold;
        }
        .top-panel .id {
            font-size: 1rem;
        }
        .address-panel {
            display: flex;
            padding: 20px;
            border-bottom: 1px solid var(--clr-border);
        }
        .address-panel div {
            flex: 1;
            padding: 0 10px;
        }
        .address-panel h4 {
            margin: 0 0 8px;
            font-size: 0.95rem;
            color: var(--clr-accent);
        }
        .details-panel {
            padding: 0 20px 20px;
            font-size: 0.9rem;
        }
        .details-panel strong {
            display: inline-block;
            width: 180px;
        }
        .line-items {
            width: 100%;
            border-collapse: collapse;
            margin: 0 auto 10px;
        }
        .line-items th, .line-items td {
            padding: 10px 14px;
            border-bottom: 1px solid var(--clr-border);
            text-align: left;
            font-size: 0.9rem;
        }
        .line-items th {
            background: var(--clr-highlight);
            font-weight: 600;
        }
        .line-items tbody tr:nth-child(even) {
            background: #fafbff;
        }
        .comment-section {
            padding: 16px 20px;
            border-top: 1px solid var(--clr-border);
            background: #fafbfc;
        }
        .comment-section h4 {
            margin: 0 0 8px;
            color: var(--clr-accent);
            font-size: 1rem;
        }
        .comment-section p {
            margin: 4px 0;
            font-size: 0.9rem;
            color: #333;
            white-space: pre-line;
        }
        .footer {
            text-align: right;
            padding: 10px 20px;
            border-top: 1px solid var(--clr-border);
            font-size: 0.9rem;
            color: #555;
        }
    </style>
</head>
<body>
<div class="refund-container">
    <!-- En-tête -->
    <div class="top-panel">
        <div class="logo">
            <div class="logo">            <img src="data:image/jpeg;base64," alt="Logo" style="max-width: 150px; height: auto;"></div>
        </div>
        <div class="middle">Centre culturel Ibn Badis Grenoble</div>
        <div class="id">Remboursement N° {{ refund.id }}</div>
    </div>
    <!-- Adresses -->
    <div class="address-panel">
        <div class="from">
            <h4>Émetteur</h4>
            <p>Centre culturel Ibn Badis Grenoble</p>
            <p>18 Rue des Trembles</p>
            <p>38100 Grenoble</p>
        </div>
        <div class="to">
            <h4>Parent</h4>
            <p><strong>{{ parentFullName }}</strong></p>
            {% if refund.parent.emailContact %}
                <p>✉️ {{ refund.parent.emailContact }}</p>
            {% endif %}
            {% set phone = refund.parent.phoneContact ?? refund.parent.fatherPhone ?? refund.parent.motherPhone %}
            {% if phone %}
                <p>☎️ {{ phone }}</p>
            {% endif %}
        </div>
    </div>
    <!-- Détails -->
    <div class="details-panel">
        <p><strong>Date de remboursement :</strong> {{ refund.refundDate ? refund.refundDate|date('d/m/Y') : '—' }}</p>
        <p><strong>Montant remboursé :</strong> {{ (refund.amount ?? 0)|number_format(2, ',', ' ') }} €</p>
        <p><strong>Méthode :</strong> {{ refund.method ?? '—' }}</p>
        <p><strong>Référence :</strong> {{ refund.reference ?? '—' }}</p>
    </div>
    <!-- Factures associées -->
    {% if refund.invoices is not empty %}
        <table class="line-items">
            <thead>
            <tr>
                <th>Facture</th>
                <th>Date</th>
                <th>Montant total</th>
                <th>Total payé</th>
                <th>Commentaire</th>
            </tr>
            </thead>
            <tbody>
            {% for inv in refund.invoices %}
                {% set totalPaid = 0 %}
                {% if inv.payments is defined and inv.payments is not empty %}
                    {% for p in inv.payments %}
                        {% set totalPaid = totalPaid + (p.amountPaid ?? 0) %}
                    {% endfor %}
                {% endif %}
                <tr>
                    <td>#{{ inv.id }}</td>
                    <td>{{ inv.invoiceDate ? inv.invoiceDate|date('d/m/Y') : '—' }}</td>
                    <td>{{ (inv.totalAmount ?? 0)|number_format(2, ',', ' ') }} €</td>
                    <td>{{ totalPaid|number_format(2, ',', ' ') }} €</td>
                    <td>{{ inv.comment ?? '—' }}</td>
                </tr>
            {% endfor %}
            </tbody>
        </table>
    {% else %}
        <p style="padding: 0 20px; color: #555;">Aucune facture associée à ce remboursement.</p>
    {% endif %}
    <!-- Raison du remboursement -->
    {% if refund.comment %}
        <div class="comment-section">
            <h4>Raison du remboursement :</h4>
            <p>{{ refund.comment }}</p>
        </div>
    {% endif %}
    <!-- Pied de page -->
    <div class="footer">
        Fait à Grenoble, le {{ "now"|date("d/m/Y") }}
    </div>
</div>
</body>
</html>